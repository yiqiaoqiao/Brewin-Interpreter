(tclass node (field_type)
  (field node@field_type next null)
  (field field_type value)
  (method void set_val ((field_type v)) (set value v))
  (method field_type get_val () (return value))
  (method void set_next((node@field_type n)) (set next n))
  (method node@field_type get_next() (return next))
)

(class temp_class
  (field node@int a null)
  (method node@int return_tclass ( (node@int tclass_param) )
      (let ( (node@int b null) )
          (print "a: " a)
          (print "b: " b)
          #(print "tclass_param: " tclass_param)
          (return tclass_param)
      )
  )
)

(class main
  (field node@int y null)
  (method void main ()
    (let ((node@int x null) (temp_class temp_obj null) )
      (print "y is " y)
      (set x (new node@int))
      (set temp_obj (new temp_class) )
      (call temp_obj return_tclass x)
      (call x set_val 5)
      (print (call x get_val))
    )
  )
)